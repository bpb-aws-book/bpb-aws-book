{
   "Version": "2012-10-17",
   "Statement": [
     {
       "Sid": "DenyVPCAccess",
       "Principal": "*",
       "Action": "s3:*",
       "Effect": "Deny",
       "Resource": ["arn:aws:s3:::YOURBUCKETNAME",
                    "arn:aws:s3:::YOURBUCKETNAME/*"],
       "Condition": {
         "StringEquals": {
           "aws:SourceVpce": "YOURVPCENDPOINT"
         }
       }
     }
   ]
}
